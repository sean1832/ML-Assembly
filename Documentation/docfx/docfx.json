{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Rhino-Grasshopper/_Scripts/TimberAssemblyLib/src/TimberAssembly/**.csproj"
          ],
		  "src":"../../",
		  "exclude": [
			"**/bin/**",
			"**/obj/**",
			"**_site/**"
		  ]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "separatePages",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
		  "Images/**"
        ],
		"src":"../"
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "globalMetadata": {
      "_appTitle": "TimberAssembly",
      "_appName": "TimberAssembly",
      "_appFaviconPath": "",
      "_appLogoPath": "",
      "_appFooter": "<strong>Developed by <a href='https://github.com/sean1832' style='color: lightblue !important;'>Zeke Zhang</a></strong>",
      "_copyrightFooter": "",
      "_enableSearch": true,
      "_disableSideFilter": false,
      "_enableNewTab": true,
      "_disableContribution": false,
      "_disableBreadcrumb": false
    },
    "template": [
      "default",
	    "template/singulinkfx"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}