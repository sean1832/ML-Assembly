{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Rhino-Grasshopper/_Scripts/TimberAssemblyLib/src/TimberAssembly/**.csproj"
          ],
		  "src":"../../",
		  "exclude": [
			"**/bin/**",
			"**/obj/**",
			"**_site/**"
		  ]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
		  "Images/**"
        ],
		"src":"../"
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}